node {
    stage "Get git code - git clone url"

    sh "echo 'Git clone complete'"
    
    stage "Run test code - mvn test"
    
    sh "echo 'Test succeded'"
    
    stage "Build Code - mvn install"
    
    sh "echo 'Build Succeded'"
    
    stage "Deploy built jar"
    
    sh "echo 'Deployed to target server'"
    
     sh "echo 'I am from github'"
    
     sh "echo 'I am polling 1 min test'"
    
}
